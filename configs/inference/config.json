{
    "n2mnet": {
        "encoder": {
            "name": "PointBERT",
            "config": {
                "NAME": "PointTransformer",
                "trans_dim": 384,
                "depth": 12,
                "drop_path_rate": 0.1,
                "cls_dim": 40,
                "num_heads": 6,
                "group_size": 32,
                "num_group": 512,
                "encoder_dims": 256
            }
        },
        "decoder": {
            "name": "mlp",
            "config": {
                "dropout": 0.1
            },
            "layers": [
                512,
                512,
                512,
                512,
                512,
                512,
                512,
                512
            ],
            "num_gaussians": 2,
            "output_dim": 3
        },
        "ckpt": "models/N2M/N2Mnet.pth"
    },
    "preprocess": {
        "pointnum": 8192
    },
    "postprocess": {
        "num_samples": 100,
        "collision_checker": {
            "ground_z": 0.05,
            "filter_noise": true,
            "resolution": 0.02,
            "robot_width": 0.5,
            "robot_length": 0.63
        }
    }
}