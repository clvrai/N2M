{
    "exp_name": "N2M_training",
    "n2mnet": {
        "encoder": {
            "name": "PointBERT",
            "config": {
                "NAME": "PointTransformer",
                "trans_dim": 384,
                "depth": 12,
                "drop_path_rate": 0.1,
                "cls_dim": 40,
                "num_heads": 6,
                "group_size": 32,
                "num_group": 512,
                "encoder_dims": 256
            }
        },
        "decoder": {
            "name": "mlp",
            "config": {
                "dropout": 0.1
            },
            "num_gaussians": 1,
            "output_dim": 3
        }
    },
    "train": {
        "dataset": {
            "dataset_path": null,
            "anno_path": "meta_aug.json",
            "pointnum": 8192,
            "train_val_ratio": 0.9,
            "augmentations": {
                "rotation_se2": {
                    "min_angle": -180,
                    "max_angle": 180
                },
                "translation_se2": {
                    "radius": 1
                }
            }
        },
        "encoder": {
            "ckpt": null,
            "freeze": false
        },
        "batch_size": 32,
        "num_epochs": 300,
        "learning_rate": 1e-4,
        "num_workers": 8,
        "val_freq": 50,
        "loss": {
            "name": "mle",
            "config": {
                "lam_weight": 0.1,
                "lam_dist": 0.1
            }
        },
        "wandb": {
            "name": null,
            "project": null,
            "entity": null
        },
        "output_dir": null
    },
    "n2mmodule": {
        "ckpt": null,
        "preprocess": {
            "pointnum": 8192
        },
        "postprocess": {
            "num_samples": 100,
            "collision_checker": {
                "ground_z": 0.05,
                "filter_noise": true,
                "resolution": 0.02,
                "robot_width": 0.5,
                "robot_length": 0.63
            }
        }
    }
}